<div class="logincontentparent">
  {{#if failedToLogin}}
  <h5>You entered the wrong username or password.</h5>
  {{/if}}
  <div class="logincontentchild">
<form action="" method="post">

  <div class="form-outline mb-4">
    <input type="text" name="username" id="form2Example1" class="form-control" />
    <label class="form-label" for="form2Example1">User Name</label>
  </div>

  <div class="form-outline mb-4">
    <input type="password" name="password" id="form2Example2" class="form-control" />
    <label class="form-label" for="form2Example2">Password</label>
  </div>

  <div class="row mb-4">
    <div class="col d-flex justify-content-center">

      <div class="form-check">
        <input
          class="form-check-input"
          type="checkbox"
          value=""
          id="form2Example31"
          checked
        />
        <label class="form-check-label" for="form2Example31">
        Remember me
        </label>
      </div>
    </div> 

    <div class="col">

      <a href="#!">Forgot password?</a>
    </div>
  </div>


  <button type="submit" value="Login" id="submitbutton" class="btn btn-dark btn-block mb-4">SIGN IN</button>


  </div>
</form>
</div>
</div>